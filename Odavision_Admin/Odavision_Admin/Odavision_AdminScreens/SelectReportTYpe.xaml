				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform AngleY="-0.115"/>
					<RotateTransform/>
					<TranslateTransform Y="-0.331"/>
				</TransformGroup>
			</TextBlock.RenderTransform>
			<i:Interaction.Triggers>
